# {{ ansible_managed }}

[Unit]
Description=Kubernetes Scheduler
Documentation=https://kubernetes.io/docs

[Service]
ExecStart=/usr/local/bin/kube-scheduler \
  --leader-elect=true \
  --kubeconfig=/etc/kubernetes/kube-scheduler.kubeconfig \
  --v=2
Restart=on-failure
RestartSec=5

[Install]
WantedBy=multi-user.target
